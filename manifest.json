{
  "name": "FreeRiceDict",
  "version": "0.1.1",
  "description": "Easily see extended definitions for words on FreeRice.com",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": { "128": "img/book.png" },
  "content_scripts": [
    {
      "matches": [
        "*://*.freerice.com/*"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
